local function JXHU_fake_script() -- Music2.LocalScript 
	local script = Instance.new('LocalScript', Music2)

	local btn = script.Parent
	
	btn.MouseButton1Click:Connect(function()
		local soundId1 = "rbxassetid://102773194760322"
		local soundId2 = "rbxassetid://128795246571917"
	
		local function playMultipleCopies(soundId, count)
			for i = 1, count do
				local sound = Instance.new("Sound", workspace)
				sound.SoundId = soundId
				sound.Volume = 10
				sound.Pitch = 0.3
				sound:Play()
				game.Debris:AddItem(sound, 30)
			end
		end
	
		coroutine.wrap(function()
			playMultipleCopies(soundId1, 100)
			wait(27.6)
			playMultipleCopies(soundId2, 100)
		end)()
	end)
end
